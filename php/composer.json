{
    "name": "microsoft/sample-app",
    "description": "Sample app using Microsoft Graph SDK",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Microsoft\\SampleApp\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Philip Gichuhi",
            "email": "pgichuhi@microsoft.com"
        }
    ],
    "require": {
		"php": "^7.4 || ^8.0",
        "microsoft/microsoft-graph": "^2.14.0",
		"microsoft/microsoft-graph-beta": "^2.16.0"
	},
	"require-dev": {
        "phpunit/phpunit": "^9.6.20",
        "phpstan/phpstan": "^1.12.4"
    },
	"config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}
